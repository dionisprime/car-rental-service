{
    "env": {
        "commonjs": true,
        "es2021": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "airbnb", "prettier"],
    "overrides": [],

    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["jest"],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": ["error", { "allow": ["warn", "error", "info"] }],
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
    }
}
